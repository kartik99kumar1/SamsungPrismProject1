<?xml version="1.0" encoding="utf-8"?>
<data-extraction-rules>
    <cloud-backup>
        <!-- Include everything -->
        <include domain="file" path="*"/>
        <include domain="database" path="*"/>
        <include domain="shared_prefs" path="*"/>
        <include domain="cache" path="*"/>
    </cloud-backup>
</data-extraction-rules>
